{
  "canisters": {
    "sns_governance": {
      "type": "custom",
      "candid": "https://raw.githubusercontent.com/dfinity/ic/master/rs/sns/governance/canister/governance.did",
      "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/sns-governance-canister.wasm.gz"
    },
    "sns_ledger": {
      "type": "custom",
      "candid": "https://raw.githubusercontent.com/dfinity/ic/master/rs/ledger_suite/icrc1/ledger/ledger.did",
      "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/ic-icrc1-ledger.wasm.gz"
    },
    "sns_root": {
      "type": "custom",
      "candid": "https://raw.githubusercontent.com/dfinity/ic/master/rs/sns/root/canister/root.did",
      "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/sns-root-canister.wasm.gz"
    },
    "sns_swap": {
      "type": "custom",
      "candid": "https://raw.githubusercontent.com/dfinity/ic/master/rs/sns/swap/canister/swap.did",
      "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/sns-swap-canister.wasm.gz"
    },
    "sns_index": {
      "type": "custom",
      "candid": "https://raw.githubusercontent.com/dfinity/ic/master/rs/ledger_suite/icrc1/index-ng/index-ng.did",
      "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/ic-icrc1-index-ng.wasm.gz"
    },
    "ckbtc_ledger": {
      "type": "custom",
      "candid": "https://raw.githubusercontent.com/dfinity/ic/master/rs/ledger_suite/icrc1/ledger/ledger.did",
      "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/ic-icrc1-ledger.wasm.gz",
      "init_arg": "(variant { Init = record { token_name = \"ckBTC\"; token_symbol = \"ckBTC\"; decimals = opt 8; minting_account = record { owner = principal \"aaaaa-aa\" }; transfer_fee = 10; metadata = vec {}; initial_balances = vec { record { record { owner = principal \"2vxsx-fae\"; subaccount = null }; 100000000 } }; archive_options = record { trigger_threshold = 2000; num_blocks_to_archive = 1000; controller_id = principal \"aaaaa-aa\" }; feature_flags = opt record { icrc2 = true }; } })"
    },
    "ckusdc_ledger": {
      "type": "custom",
      "candid": "https://raw.githubusercontent.com/dfinity/ic/master/rs/ledger_suite/icrc1/ledger/ledger.did",
      "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/ic-icrc1-ledger.wasm.gz",
      "init_arg": "(variant { Init = record { token_name = \"ckUSDC\"; token_symbol = \"ckUSDC\"; decimals = opt 6; minting_account = record { owner = principal \"aaaaa-aa\" }; transfer_fee = 10000; metadata = vec {}; initial_balances = vec { record { record { owner = principal \"2vxsx-fae\"; subaccount = null }; 100000000 } }; archive_options = record { trigger_threshold = 2000; num_blocks_to_archive = 1000; controller_id = principal \"aaaaa-aa\" }; feature_flags = opt record { icrc2 = true }; } })"
    },
    "data_canister": {
      "type": "rust",
      "package": "data_canister",
      "candid": "canisters/data_canister/data_canister.did",
      "declarations": {
        "bindings": ["rust"],
        "output": "canisters/bindings"
      }
    },
    "crypto_canister": {
      "type": "rust",
      "package": "crypto_canister",
      "candid": "canisters/crypto_canister/crypto_canister.did"
    },
    "agent_canister": {
      "type": "rust",
      "package": "agent_canister",
      "candid": "canisters/agent_canister/agent_canister.did"
    },
    "ussd_canister": {
      "type": "rust",
      "package": "ussd_canister",
      "candid": "canisters/ussd_canister/ussd_canister.did"
    },
    "satellite": {
      "type": "custom",
      "candid": "src/satellite/satellite.did",
      "build": "cd src/satellite && cargo build --target wasm32-unknown-unknown --release",
      "wasm": "src/satellite/target/wasm32-unknown-unknown/release/satellite.wasm"
    }
  },
  "networks": {
    "juno": {
      "providers": ["http://127.0.0.1:5987"],
      "type": "persistent"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
