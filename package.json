{
  "name": "afritokeni",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "svelte-kit sync && vite build",
    "canisters:build": "cargo build --target wasm32-unknown-unknown --release --package data_canister --package user_canister --package wallet_canister --package agent_canister --package crypto_canister --package ussd_canister",
    "canisters:deploy": "npm run canisters:build && dfx deploy data_canister && dfx deploy user_canister && dfx deploy wallet_canister && dfx deploy agent_canister && dfx deploy crypto_canister && dfx deploy ussd_canister",
    "canisters:generate": "npm run canisters:build && npm run canisters:generate-candid && npm run canisters:generate-ts",
    "canisters:generate-candid": "candid-extractor target/wasm32-unknown-unknown/release/data_canister.wasm > canisters/data_canister/data_canister.did && candid-extractor target/wasm32-unknown-unknown/release/user_canister.wasm > canisters/user_canister/user_canister.did && candid-extractor target/wasm32-unknown-unknown/release/wallet_canister.wasm > canisters/wallet_canister/wallet_canister.did && candid-extractor target/wasm32-unknown-unknown/release/agent_canister.wasm > canisters/agent_canister/agent_canister.did && candid-extractor target/wasm32-unknown-unknown/release/crypto_canister.wasm > canisters/crypto_canister/crypto_canister.did && candid-extractor target/wasm32-unknown-unknown/release/ussd_canister.wasm > canisters/ussd_canister/ussd_canister.did",
    "canisters:generate-ts": "dfx generate data_canister && dfx generate user_canister && dfx generate wallet_canister && dfx generate agent_canister && dfx generate crypto_canister && dfx generate ussd_canister",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "data:seed": "tsx scripts/primeDemoData.ts",
    "deploy:local": "bash scripts/deployment/deploy.sh local",
    "deploy:mainnet": "bash scripts/deployment/deploy.sh ic",
    "deploy:playground": "bash scripts/deployment/deploy.sh playground",
    "deploy:setup-auth:local": "bash scripts/deployment/setup-auth.sh local",
    "deploy:setup-auth:mainnet": "bash scripts/deployment/setup-auth.sh ic",
    "deploy:setup-auth:playground": "bash scripts/deployment/setup-auth.sh playground",
    "deploy:test:local": "bash scripts/deployment/test-deployment.sh local",
    "deploy:test:mainnet": "bash scripts/deployment/test-deployment.sh ic",
    "deploy:test:playground": "bash scripts/deployment/test-deployment.sh playground",
    "dev": "vite dev",
    "format": "prettier --write \"src/**/*.{ts,tsx,svelte,js,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,svelte,js,json,css}\"",
    "juno:dev:login": "juno login --mode development --emulator",
    "juno:dev:publish": "juno config apply -m development",
    "juno:dev:start": "juno dev start",
    "juno:dev:stop": "juno dev stop",
    "juno:emulator:deploy": "juno deploy --mode test",
    "juno:emulator:start": "juno dev start --mode test",
    "juno:emulator:stop": "juno dev stop",
    "lint": "eslint . --ext .ts,.tsx,.svelte",
    "lint:fix": "eslint . --ext .ts,.tsx,.svelte --fix",
    "ops:admin:add": "bash scripts/ops/admin.sh add-admin",
    "ops:admin:remove": "bash scripts/ops/admin.sh remove-admin",
    "ops:destroy:local": "dfx stop || true",
    "ops:status:local": "bash scripts/ops/admin.sh status local",
    "ops:status:mainnet": "bash scripts/ops/admin.sh status ic",
    "ops:status:playground": "bash scripts/ops/admin.sh status playground",
    "ops:upgrade:local": "bash scripts/ops/admin.sh upgrade local",
    "ops:upgrade:mainnet": "bash scripts/ops/admin.sh upgrade ic",
    "ops:upgrade:playground": "bash scripts/ops/admin.sh upgrade playground",
    "postinstall": "npm run postinstall:copy-auth",
    "postinstall:copy-auth": "node -e \"require('fs').cpSync('node_modules/@junobuild/core/dist/workers/', './public/workers', {recursive: true});\"",
    "precommit": "npm run format && npm run lint:fix && npm run check",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "npm run test:unit && npm run test:integration && npm run test:frontend",
    "test:coverage": "cargo llvm-cov --workspace --tests --summary-only",
    "test:frontend": "vitest run",
    "test:frontend:watch": "vitest",
    "test:frontend:ui": "vitest --ui",
    "test:frontend:coverage": "vitest run --coverage",
    "test:integration": "npm run test:integration:all",
    "test:integration:all": "cargo test --test lib --manifest-path canisters/data_canister/Cargo.toml -- --test-threads=1 && cargo test --test lib --manifest-path canisters/user_canister/Cargo.toml -- --test-threads=1 && cargo test --test lib --manifest-path canisters/wallet_canister/Cargo.toml -- --test-threads=1 && cargo test --test lib --manifest-path canisters/agent_canister/Cargo.toml -- --test-threads=1 && cargo test --test lib --manifest-path canisters/crypto_canister/Cargo.toml -- --test-threads=1 && cargo test --test lib --features test-utils --manifest-path canisters/ussd_canister/Cargo.toml -- --test-threads=1",
    "test:integration:data": "cargo test --test lib --manifest-path canisters/data_canister/Cargo.toml -- --test-threads=1",
    "test:integration:user": "cargo test --test lib --manifest-path canisters/user_canister/Cargo.toml -- --test-threads=1",
    "test:integration:wallet": "cargo test --test lib --manifest-path canisters/wallet_canister/Cargo.toml -- --test-threads=1",
    "test:integration:agent": "cargo test --test lib --manifest-path canisters/agent_canister/Cargo.toml -- --test-threads=1",
    "test:integration:crypto": "cargo test --test lib --manifest-path canisters/crypto_canister/Cargo.toml -- --test-threads=1",
    "test:integration:ussd": "cargo test --test lib --features test-utils --manifest-path canisters/ussd_canister/Cargo.toml -- --test-threads=1",
    "test:unit": "cargo test --workspace --lib --features ussd_canister/test-utils",
    "validate": "npm run format:check && npm run lint && npm run check"
  },
  "dependencies": {
    "@dfinity/agent": "3.4.1",
    "@dfinity/auth-client": "3.4.1",
    "@dfinity/candid": "3.4.1",
    "@dfinity/identity": "3.4.1",
    "@dfinity/ledger-icrc": "4.1.2",
    "@dfinity/principal": "3.4.1",
    "@dfinity/sns": "^5.0.0",
    "@dfinity/utils": "3.2.0",
    "@flowbite-svelte-plugins/chart": "^0.2.4",
    "@junobuild/core": "2.2.1",
    "@junobuild/functions": "0.3.3",
    "@lucide/svelte": "^0.552.0",
    "@tailwindcss/typography": "0.5.19",
    "@types/qrcode": "1.5.6",
    "@userback/widget": "0.3.11",
    "africastalking": "^0.7.7",
    "apexcharts": "^5.3.6",
    "chart.js": "^4.5.1",
    "ethers": "^6.15.0",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "^1.23.0",
    "flowbite-svelte-icons": "^3.0.0",
    "leaflet": "1.9.4",
    "marked": "^16.4.1",
    "nanoid": "5.1.5",
    "qrcode": "1.5.4",
    "rehype-raw": "7.0.0",
    "remark-gfm": "4.0.1",
    "svelte-chartjs": "^3.1.5",
    "svelte-leafletjs": "^2.0.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@dfinity/internet-identity-playwright": "^2.0.0",
    "@eslint/js": "9.39.0",
    "@junobuild/config": "2.5.0",
    "@junobuild/vite-plugin": "4.3.0",
    "@playwright/test": "^1.56.1",
    "@rollup/plugin-yaml": "^4.1.2",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "2.48.4",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@tailwindcss/postcss": "4.1.16",
    "@tailwindcss/vite": "4.1.16",
    "@types/africastalking": "^0.6.2",
    "@types/leaflet": "1.9.21",
    "@types/node": "24.10.0",
    "@vitest/coverage-v8": "4.0.6",
    "@vitest/ui": "4.0.6",
    "eslint": "9.39.0",
    "globals": "16.2.0",
    "happy-dom": "20.0.10",
    "husky": "^9.0.11",
    "postcss": "8.5.6",
    "prettier": "3.6.1",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "0.6.13",
    "svelte": "5.43.2",
    "svelte-check": "4.3.3",
    "tailwindcss": "4.1.16",
    "ts-node": "^10.9.2",
    "tsx": "4.20.5",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0",
    "vite": "7.1.9",
    "vite-plugin-node-polyfills": "0.24.0",
    "vite-plugin-top-level-await": "1.6.0",
    "vite-plugin-wasm": "3.5.0",
    "vitest": "4.0.6"
  },
  "overrides": {
    "vite-plugin-node-polyfills": {
      "vite": "6.0.0"
    }
  }
}
