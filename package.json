{
  "name": "afritokeni",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "juno:dev:start": "juno dev start",
    "juno:dev:stop": "juno dev stop",
    "juno:dev:publish": "juno config apply -m development",
    "juno:dev:login": "juno login --mode development --emulator",
    "data:seed": "tsx scripts/primeDemoData.ts",
    "test:unit": "NODE_ENV=unit-test TSX_TSCONFIG_PATH=./tsconfig.test.json cucumber-js 'tests/unit/*.feature' --require 'tests/unit/setup.ts' --require 'tests/unit/*.ts' --require-module tsx",
    "test:integration": "npm run icp:start && npm run icp:deploy && NODE_ENV=integration DFX_NETWORK=local TSX_TSCONFIG_PATH=./tsconfig.test.json cucumber-js 'tests/integration/features/*.feature' --require 'tests/integration/setup.ts' --require 'tests/integration/steps/*.ts' --require-module tsx",
    "test:all": "npm run test:unit && npm run test:integration",
    "icp:start": "dfx start --clean --background",
    "icp:stop": "dfx stop",
    "icp:deploy": "dfx deploy ckbtc_ledger && dfx deploy ckusdc_ledger",
    "canisters:build": "cargo build --target wasm32-unknown-unknown --release --package deposit_canister --package withdrawal_canister --package exchange_canister",
    "canisters:generate-candid": "candid-extractor target/wasm32-unknown-unknown/release/deposit_canister.wasm > canisters/deposit_canister/deposit_canister.did && candid-extractor target/wasm32-unknown-unknown/release/withdrawal_canister.wasm > canisters/withdrawal_canister/withdrawal_canister.did && candid-extractor target/wasm32-unknown-unknown/release/exchange_canister.wasm > canisters/exchange_canister/exchange_canister.did",
    "canisters:generate-ts": "dfx generate deposit_canister && dfx generate withdrawal_canister && dfx generate exchange_canister && mv src/declarations/deposit_canister/deposit_canister.did.d.ts src/lib/services/icp/canisters/depositCanister.ts && mv src/declarations/withdrawal_canister/withdrawal_canister.did.d.ts src/lib/services/icp/canisters/withdrawalCanister.ts && mv src/declarations/exchange_canister/exchange_canister.did.d.ts src/lib/services/icp/canisters/exchangeCanister.ts",
    "canisters:deploy": "npm run canisters:build && dfx deploy deposit_canister && dfx deploy withdrawal_canister && dfx deploy exchange_canister",
    "canisters:generate": "npm run canisters:build && npm run canisters:generate-candid && npm run canisters:generate-ts",
    "lint": "eslint . --ext .ts,.tsx,.svelte",
    "lint:fix": "eslint . --ext .ts,.tsx,.svelte --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,svelte,js,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,svelte,js,json,css}\"",
    "validate": "npm run format:check && npm run lint && npm run check && npm run test:unit",
    "precommit": "npm run format && npm run lint:fix && npm run check",
    "prepare": "husky install",
    "postinstall": "npm run postinstall:copy-auth",
    "postinstall:copy-auth": "node -e \"require('fs').cpSync('node_modules/@junobuild/core/dist/workers/', './public/workers', {recursive: true});\""
  },
  "dependencies": {
    "@dfinity/agent": "3.4.1",
    "@dfinity/auth-client": "3.4.1",
    "@dfinity/candid": "3.4.1",
    "@dfinity/identity": "3.4.1",
    "@dfinity/ledger-icrc": "4.1.2",
    "@dfinity/nns": "10.4.0",
    "@dfinity/principal": "3.4.1",
    "@dfinity/sns": "4.1.2",
    "@dfinity/utils": "3.2.0",
    "@dfinity/zod-schemas": "2.1.0",
    "@faker-js/faker": "10.1.0",
    "@junobuild/core": "2.2.1",
    "@junobuild/functions": "0.3.3",
    "@lucide/svelte": "^0.552.0",
    "@tailwindcss/typography": "0.5.19",
    "@types/bitcoinjs-lib": "4.0.1",
    "@types/qrcode": "1.5.6",
    "@userback/widget": "0.3.11",
    "africastalking": "0.7.4",
    "dotenv": "16.6.1",
    "ethers": "5.8.0",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "^1.22.0",
    "flowbite-svelte-icons": "^3.0.0",
    "leaflet": "1.9.4",
    "lucide-svelte": "0.552.0",
    "marked": "^16.4.1",
    "nanoid": "5.1.5",
    "qrcode": "1.5.4",
    "rehype-raw": "7.0.0",
    "remark-gfm": "4.0.1",
    "resend": "6.0.3",
    "svelte-leafletjs": "^2.0.0",
    "tiny-secp256k1": "2.2.4",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@cucumber/cucumber": "11.3.0",
    "@eslint/js": "9.39.0",
    "@junobuild/config": "2.5.0",
    "@junobuild/vite-plugin": "4.3.0",
    "@rollup/plugin-yaml": "^4.1.2",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "2.48.4",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@tailwindcss/postcss": "4.1.16",
    "@tailwindcss/vite": "4.1.16",
    "@types/africastalking": "0.6.2",
    "@types/leaflet": "1.9.21",
    "@types/node": "24.10.0",
    "@types/node-fetch": "2.6.13",
    "@vitest/coverage-v8": "4.0.6",
    "@vitest/ui": "4.0.6",
    "eslint": "9.39.0",
    "globals": "16.2.0",
    "happy-dom": "20.0.10",
    "husky": "^9.0.11",
    "msw": "2.7.0",
    "node-fetch": "2.7.0",
    "postcss": "8.5.6",
    "prettier": "3.6.1",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "0.6.13",
    "svelte": "5.43.2",
    "svelte-check": "4.3.3",
    "tailwindcss": "4.1.16",
    "tsx": "4.20.5",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0",
    "vite": "7.1.9",
    "vite-plugin-node-polyfills": "0.24.0",
    "vite-plugin-top-level-await": "1.6.0",
    "vite-plugin-wasm": "3.5.0",
    "vitest": "4.0.6"
  },
  "overrides": {
    "vite-plugin-node-polyfills": {
      "vite": "6.0.0"
    }
  }
}
